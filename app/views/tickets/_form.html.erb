<%= form_for([@venue, @ticket]) do |f| %> 
    <% if !@ticket.venue %> 
    <%= f.label :venue %>
    <%= f.collection_select :venue_id, current_user.venues.all, :id, :name %> Or <%= link_to "Add a new Venue", new_venue_path %>
    <% else %>
        <%= f.hidden_field :venue_id %>
    <% end %>
    <br>
    <br>
    <%= f.label :name %> 
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :date %> 
    <%= f.text_field :date %>
    <br>
    <br>
    <%= f.label :artist %> 
    <%= f.text_field :artist %>
    <br>
    <br>
    <%= f.submit "Submit" %>
    <br>
<% end %>