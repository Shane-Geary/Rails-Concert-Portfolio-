<h1>Add a new Ticket</h1>

<%= form_for(@ticket) do |f| %>
    <%= f.label :name %> 
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :date %>
    <%= f.text_field :date %>
    <br>
    <br>
    <%= f.label :artist %> 
    <%= f.text_field :artist %>
    <br>
    <br>
    <%= f.submit "Submit " %>
<% end %>